  include ../includes/banner.pug
  +banner('Exclusive Listings', 'numy-exclusive-mobile.png', 'numy-exclusive-tablet.png', 'numy-exclusive.png')

  //- Breadcrumbs Section
  include ../includes/breadcrumbs.pug
  +breadcrumbs('/exclusive-listings.html', pageTitle)

